{# The User List page #}
{% extends 'base_nav.html.jinja' %}
{% block content %}
  {% if users|length == 0 %}
    <h3>Sorry, no users found</h3>
  {% else %}
    <div class="row justify-content-end">
      <div class="col-sm-9">
        <div class="row">
          {% for list_user in users %}
            {% include "/users/_list_card.html.jinja" %}
          {% endfor %}
        </div>
      </div>
    </div>
  {% endif %}
{% endblock %}